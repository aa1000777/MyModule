<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_circle_image_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.cjj.mytoolsapplication.circleImageView.CircleImageViewActivity">

    <com.cjj.base.ui.CircleImageView
        android:id="@+id/iv_civ1"
        android:layout_width="80.0dip"
        android:layout_height="80.0dip"
        android:clickable="true"
        android:padding="1.0dip"
        android:src="@drawable/image" />

    <com.cjj.base.ui.CircleImageView
        android:id="@+id/iv_civ2"
        android:layout_width="80.0dip"
        android:layout_height="80.0dip"
        android:layout_toRightOf="@+id/iv_civ1"
        android:clickable="true"
        android:padding="1.0dip"
        android:src="@drawable/image"
        app:border_color="@android:color/holo_red_dark"
        app:border_width="2dp" />

    <com.cjj.roundedimageviewlibrary.RoundedImageView
        android:id="@+id/iv_riv1"
        android:layout_width="80.0dip"
        android:layout_height="80.0dip"
        android:layout_below="@+id/iv_civ1"
        android:scaleType="centerCrop"
        android:src="@drawable/image"
        app:riv_corner_radius="5.0dip" />

    <com.cjj.roundedimageviewlibrary.RoundedImageView
        android:id="@+id/iv_riv2"
        android:layout_width="80.0dip"
        android:layout_height="80.0dip"
        android:layout_below="@+id/iv_civ2"
        android:layout_toRightOf="@+id/iv_riv1"
        android:background="@drawable/git_start"
        android:scaleType="fitCenter"
        android:src="@drawable/image"
        app:riv_border_color="#333333"
        app:riv_border_width="2dip"
        app:riv_corner_radius="30dip"
        app:riv_mutate_background="true"
        app:riv_oval="true" />

    <com.cjj.roundedimageviewlibrary.RoundedImageView
        android:id="@+id/iv_riv3"
        android:layout_width="80.0dip"
        android:layout_height="80.0dip"
        android:layout_below="@+id/iv_civ2"
        android:layout_toRightOf="@+id/iv_riv2"
        android:scaleType="fitCenter"
        android:src="@drawable/image"
        app:riv_border_color="#333333"
        app:riv_border_width="2dip"
        app:riv_corner_radius="30dip"
        app:riv_oval="true" />

    <com.cjj.roundedimageviewlibrary.RoundedImageView
        android:id="@+id/iv_riv4"
        android:layout_width="80.0dip"
        android:layout_height="80.0dip"
        android:layout_below="@+id/iv_civ2"
        android:layout_toRightOf="@+id/iv_riv3"
        android:scaleType="centerCrop"
        android:src="@drawable/image"
        app:riv_border_color="#333333"
        app:riv_border_width="2dip"
        app:riv_oval="true" />

    <com.cjj.roundedimageviewlibrary.RoundedImageView
        android:id="@+id/iv_riv5"
        android:layout_width="80.0dip"
        android:layout_height="80.0dip"
        android:layout_below="@+id/iv_riv1"
        android:scaleType="centerCrop"
        android:src="@drawable/image"
        app:riv_oval="true" />

    <com.cjj.roundedimageviewlibrary.RoundedImageView
        android:id="@+id/iv_riv6"
        android:layout_width="80.0dip"
        android:layout_height="80.0dip"
        android:layout_below="@+id/iv_riv1"
        android:layout_toRightOf="@+id/iv_riv5"
        android:scaleType="centerCrop"
        android:src="@drawable/image"
        app:riv_border_color="#333333"
        app:riv_border_width="2dip"
        app:riv_corner_radius="5.0dip" />
</RelativeLayout>
